end

function _update60()
    t = time()
    x = cos(t) * t
    y = sin(t) * t
end

function _draw()
    circfill(64 + x, 64 + y, 4 + t, 0)
    circfill(64 - x, 64 - y, 4 + t, 3 + t % 3)
end

